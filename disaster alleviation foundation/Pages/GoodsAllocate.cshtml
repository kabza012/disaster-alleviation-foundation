@page
@model disaster_alleviation_foundation.Pages.GoodsAllocateModel


    @{
    ViewData["Title"] = "Allocate Goods";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="form-group">
        <label for="Disasters.DisasterId">DisasterId:</label>
        <input type="number" class="form-control" id="number" name="DisasterId" required />
    </div>
    <div class="form-group">
        <label for="Description">Goods:</label>
        <textarea class="form-control" id="Description" name="Goods" rows="4" required></textarea>
    </div>

    <!-- Add other form fields as needed -->

    <button type="submit" class="btn btn-primary">Allocate Goods</button>
</form>

